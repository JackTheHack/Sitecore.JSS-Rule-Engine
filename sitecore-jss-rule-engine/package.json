{
  "name": "sitecore-jss-rule-engine",
  "version": "0.0.43",
  "description": "Rule engine for Sitecore JSS",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "ava",
    "release": "npm run build && npm version patch && npm publish",
    "build": "npm run build:esm",
    "build:esm": "tsc",
