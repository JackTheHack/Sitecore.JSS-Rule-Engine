  "keywords": [
    "JSS",
    "Sitecore",
    "rule",
    "engine"
  ],
  "author": "Jack Spektor",
  "license": "ISC",
  "dependencies": {
    "@rgrove/parse-xml": "^4.1.0",
    "@types/node": "^20.8.9",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "ava": "^5.3.1",
    "typescript": "^5.2.2"
  }
}
